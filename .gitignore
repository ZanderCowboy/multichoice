# Ignore platform code in root directory
/android/
/ios/
/web/
/windows/

# Files and directories created by pub
.dart_tool/
.packages
build/
pubspec.lock
.flutter-plugins
.flutter-plugins-dependencies
.pub-cache/
.pub/
**/doc/api/
**/ios/Flutter/.last_build_id

# Generated Code
**/*.freezed.dart
**/*.g.dart
**/*.config.dart
**/*.auto_mappr.dart
**/*.tailor.dart
**/*.gr.dart
**/*.isar
**/*.isar.lock
**/libisar.dylib
**/coverage
**/*.mocks.dart

# Keys and Secrets
password.txt
upload-keystore.*
**/key.properties
**/multichoice-*.json
**/**/.ssh

# dotenv environment variables file
.env*
**/tmp.*
**/tmp/

# Avoid committing generated Javascript files:
*.dart.js
*.info.json      # Produced by the --dump-info flag.
*.js             # When generated by dart2js. Don't specify *.js if your
                 # project includes source files written in JavaScript.
*.js_
*.js.deps
*.js.map

# Miscellaneous
*.class
*.log
*.pyc
*.swp
.DS_Store
.atom/
.buildlog/
.history
.svn/
migrate_working_dir/
devtools_options.yaml

# IntelliJ related
*.iml
*.ipr
*.iws
.idea/
